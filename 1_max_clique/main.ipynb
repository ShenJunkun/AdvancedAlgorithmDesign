   "source": [
    "import networkx as nx\n",
    "\n",
    "# 创建一个简单的图\n",
    "G = nx.Graph()\n",
    "G.add_edges_from([(1, 2), (2, 3), (3, 4)])\n",
    "\n",
    "# 获得图的所有边的列表\n",
    "edges_list = list(G.edges())\n",
    "G.add_edge(1, 4)\n",
    "# 打印边的列表\n",
    "print(\"Edges List:\", edges_list)\n"
   ]
  }
